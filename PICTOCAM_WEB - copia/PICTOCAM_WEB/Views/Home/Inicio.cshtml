@using (Html.BeginForm ("Login","Home", FormMethod.Post))
{
    <div class="modal fade customModal" id="loginModal" tabindex="-1" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="panel panel-default formPanel">
                    <div class="panel-heading" style="background-color:#041319;">
                        <h3 class="panel-title">Login</h3>
                    </div>
                    <div class="panel-body">
                        <form action="#" method="POST" role="form">
                            <div class="form-group formField">
                                @Html.TextBoxFor(model => model.Correo, new { @class= "form-control", @placeholder= "Correo", @type = "email" })
                                @*<input type="email" class="form-control" placeholder="Correo" id="correo">*@
                            </div>
                            <div class="form-group formField">
                                @Html.TextBoxFor(model => model.Contrasenia, new { @class = "form-control", @placeholder = "Contraseña", @type = "password" })
                                @*<input type="password" class="form-control" placeholder="Contraseña" id="comtrasenia">*@
                            </div>
                            <div class="form-group formField">
                                <input type="submit" class="btn btn-block" style="color:white; background-color:#041319;" value="Log in" />
                            </div>
                            <div class="form-group formField">
                                <p class="help-block"><a href="#" style="color:#041319;">Forgot password?</a></p>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>

}

@if (TempData["Mensaje"] != null)
{
    <div class="alert alert-danger fade in">
        <strong>Error!</strong> @TempData["Mensaje"].ToString();
    </div>
}
